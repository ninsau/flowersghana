      </span>\n\n        ${this.closable ? T`\n              <span class=\"alert__close\">\n                <sl-icon-button\n                  exportparts=\"base:close-button\"\n                  name=\"x\"\n                  library=\"system\"\n                  @click=${this.handleCloseClick}\n                ></sl-icon-button>\n              </span>\n            ` : \"\"}\n      </div>\n    `;\n  }\n};\nSlAlert.styles = alert_styles_default;\nObject(chunk_IHGPZX35[\"b\" /* __decorateClass */])([\n  o6('[part=\"base\"]')\n], SlAlert.prototype, \"base\", 2);\nObject(chunk_IHGPZX35[\"b\" /* __decorateClass */])([\n  e4({ type: Boolean, reflect: true })\n], SlAlert.prototype, \"open\", 2);\nObject(chunk_IHGPZX35[\"b\" /* __decorateClass */])([\n  e4({ type: Boolean, reflect: true })\n], SlAlert.prototype, \"closable\", 2);\nObject(chunk_IHGPZX35[\"b\" /* __decorateClass */])([\n  e4({ reflect: true })\n], SlAlert.prototype, \"type\", 2);\nObject(chunk_IHGPZX35[\"b\" /* __decorateClass */])([\n  e4({ type: Number })\n], SlAlert.prototype, \"duration\", 2);\nObject(chunk_IHGPZX35[\"b\" /* __decorateClass */])([\n  watch(\"open\", { waitUntilFirstUpdate: true })\n], SlAlert.prototype, \"handleOpenChange\", 1);\nObject(chunk_IHGPZX35[\"b\" /* __decorateClass */])([\n  watch(\"duration\")\n], SlAlert.prototype, \"handleDurationChange\", 1);\nSlAlert = Object(chunk_IHGPZX35[\"b\" /* __decorateClass */])([\n  n5(\"sl-alert\")\n], SlAlert);\nvar alert_default = SlAlert;\nsetDefaultAnimation(\"alert.show\", {\n  keyframes: [\n    { opacity: 0, transform: \"scale(0.8)\" },\n    { opacity: 1, transform: \"scale(1)\" }\n  ],\n  options: { duration: 250, easing: \"ease\" }\n});\nsetDefaultAnimation(\"alert.hide\", {\n  keyframes: [\n    { opacity: 1, transform: \"scale(1)\" },\n    { opacity: 0, transform: \"scale(0.8)\" }\n  ],\n  options: { duration: 250, easing: \"ease\" }\n});\n\n\n\n// CONCATENATED MODULE: ./node_modules/@shoelace-style/shoelace/dist/chunks/chunk.UPJVYZIW.js